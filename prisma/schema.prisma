// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Skill {
  id   Int    @id @default(autoincrement())
  name String
}

model WorkExperience {
  id       Int    @id @default(autoincrement())
  position String
  company  String
  date     String
}

model Project {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  link        String
  image       String
}
